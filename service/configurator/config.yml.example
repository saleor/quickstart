    category: "Hybrid"
    attributes:
      - attribute: "Flower Strain"
        values: ["Hybrid"]
      - attribute: "Flower THC Content"
        values: ["20-25%"]
      - attribute: "Flower CBD Content"
        values: ["<1%"]
    channelListings:
      - channelSlug: "online-store"
